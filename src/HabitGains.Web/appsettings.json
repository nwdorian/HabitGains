{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore.StaticFiles": "Warning",
        "Microsoft.AspNetCore.StaticAssets": "Warning",
        "Microsoft.AspNetCore.Routing": "Warning",
        "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
        "System": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "Default": "Data Source = HabitGains.db"
  },
  "Seeding": {
    "SeedOnStartup": false,
    "EntriesPerHabit": 100
  },
  "AllowedHosts": "*"
}